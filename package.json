{
    "name": "teamfour",
    "version": "0.1.0",
    "description": "File Browser Project given by HPE for the CS320/F18 UMass class",
    "private": true,
    "main": "server.js",
    "dependencies": {
        "concurrently": "^4.0.1",
        "express": "^4.16.4",
        "mongoose": "^5.3.11",
        "nodemon": "^1.18.6",
        "react": "^16.6.0"
    },
    "scripts": {
        "client-install": "npm install --prefix teamfour",
        "start": "node server.js",
        "server": "nodemon server.js",
        "client": "npm start --prefix teamfour",
        "dev": "concurrently \"npm run server\" \"npm run client\""
    }
}
